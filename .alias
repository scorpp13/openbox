alias update='sudo zypper up'
alias upgrade='sudo zypper dup'
alias verify='sudo zypper ve'
alias refresh='sudo zypper ref'
alias cclean='sudo zypper cc'
alias unneed='sudo zypper pa --unneeded'
alias fupdate='flatpak update'
alias fclean='flatpak uninstall --unused --delete-data'
alias frepair='sudo flatpak repair'
alias c='clear'
alias grep='grep --colour=auto'
alias egrep='egrep --colour=auto'
alias fgrep='fgrep --colour=auto'
alias cp='cp -i'
alias ls='ls -a --color'
alias e='exit'
alias sv='sudo vifm'
